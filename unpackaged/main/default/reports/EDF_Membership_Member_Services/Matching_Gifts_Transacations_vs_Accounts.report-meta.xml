    <columns>
        <field>Contact.OpportunityContactRoles*Opportunity$Opportunity_Record_ID__c</field>
    </columns>
    <description>Looking at gifts with Matching Gifts Transacation Type that are booked to accounts that do not include &quot;Matching Gifts&quot; in the name for review and correction or exclusion, as appropriate.</description>
    <filter>
        <criteriaItems>
            <column>Contact.OpportunityContactRoles*Opportunity$Transaction_Type__c</column>
            <columnToColumn>false</columnToColumn>
            <isUnlocked>true</isUnlocked>
            <operator>equals</operator>
            <value>Matching Gift</value>
        </criteriaItems>
        <criteriaItems>
            <column>Contact.OpportunityContactRoles$Account.Name</column>
            <columnToColumn>false</columnToColumn>
            <isUnlocked>true</isUnlocked>
            <operator>notContain</operator>
            <value>Matching Gifts,Unidentified,Anonymous</value>
        </criteriaItems>
        <criteriaItems>
            <column>Contact.OpportunityContactRoles$Name</column>
            <columnToColumn>false</columnToColumn>
            <isUnlocked>true</isUnlocked>
            <operator>notContain</operator>
            <value>Bloomberg</value>
        </criteriaItems>
        <criteriaItems>
            <column>Contact$Constituency__c</column>
            <columnToColumn>false</columnToColumn>
            <isUnlocked>true</isUnlocked>
            <operator>includes</operator>
            <value>Membership</value>
        </criteriaItems>
        <criteriaItems>
            <column>Contact.OpportunityContactRoles$Account.Account_Record_ID__c</column>
            <columnToColumn>false</columnToColumn>
            <isUnlocked>true</isUnlocked>
            <operator>notEqual</operator>
            <value>ACC-7278181,ACC-3713895</value>
        </criteriaItems>
        <criteriaItems>
            <column>Contact.OpportunityContactRoles$Account.Organization_sub_constituency__c</column>
            <columnToColumn>false</columnToColumn>
            <isUnlocked>true</isUnlocked>
            <operator>excludes</operator>
            <value>Development - Major Gifts</value>
        </criteriaItems>
        <language>en_US</language>
    </filter>
    <format>Tabular</format>
