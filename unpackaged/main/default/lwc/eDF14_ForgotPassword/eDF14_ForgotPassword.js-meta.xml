<?xml version="1.0" encoding="UTF-8"?>
<LightningComponentBundle xmlns="http://soap.sforce.com/2006/04/metadata">
    <apiVersion>52.0</apiVersion>
    <isExposed>true</isExposed>
    <targets>
        <target>lightningCommunity__Page</target>
        <target>lightningCommunity__Default</target>
    </targets>
    <targetConfigs>
        <targetConfig targets="lightningCommunity__Default">
            <property name="msgPwdResetEmailSent" label="Message - Password Reset Email sent" type="String" default="We have sent a password reset link to your email."></property>
            <property name="msgPwdResetError" label="Message - Password Reset Error" type="String" default="Password reset was unsuccessful, please try again later."></property>
            <property name="msgNotRegistered" label="Message - Not Registered" type="String" default="It seems you are not registered with us. Would you like to register?"></property>

            <property name="placeholderEmailField" label="Placeholder Text - Email Field" type="String" default="Enter your registered email ID"></property>
            <property name="labelSubmit" label="Label - Submit Button" type="String" default="Submit"></property>

            <property name="titleDialog" label="Title - Message Dialog" type="String" default="Forgot Password"></property>
            <property name="btnLogin" label="Label - Login Button" type="String" default="Login"></property>
            <property name="btnRegister" label="Label - Register Button" type="String" default="Register"></property>

        </targetConfig>
    </targetConfigs>

</LightningComponentBundle>
