    <description>The transaction status shown in Elevate.</description>
    <externalId>false</externalId>
    <formula>CASE(npsp__Payment_Status__c,
                'PENDING',
                'Pending',

                'CAPTURED',
                'Succeeded',

                'NONRETRYABLEERROR',
                'Failed',

                'RETRYABLEERROR',
                'Failed',

                'DECLINED',
                'Declined',

                'REFUNDISSUED',
                'Succeeded',

                'SUBMITTED',
                'Submitted',

                'SETTLED',
                'Succeeded',

                'AUTHORIZED',
                'Authorized',

                'EXPIRED',
                'Expired Authorization',

                'PAYMENTPROCESSING',
                'Pending',

