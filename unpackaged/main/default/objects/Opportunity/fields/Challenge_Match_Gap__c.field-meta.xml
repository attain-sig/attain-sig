<?xml version="1.0" encoding="utf-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Challenge_Match_Gap__c</fullName>
    <description>If the Threshold Amount is greater than the Raised to Date for Match, this field will display the difference between Threshold Amount and Raised to Date for Match. Otherwise, it will be 0.</description>
    <externalId>false</externalId>
    <formula>If( Amount_Needed_to_Unlock_Challenge_Match__c  &gt; Raised_to_Date_for_Challenge_Match__c, Amount_Needed_to_Unlock_Challenge_Match__c  -Raised_to_Date_for_Challenge_Match__c, 0)</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <label>Challenge Match Gap</label>
    <precision>18</precision>
    <required>false</required>
    <scale>2</scale>
    <trackHistory>false</trackHistory>
    <trackTrending>false</trackTrending>
    <type>Currency</type>
</CustomField>
