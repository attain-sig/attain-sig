<?xml version="1.0" encoding="utf-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>iContactforSF__Forward_Rate__c</fullName>
    <deprecated>false</deprecated>
    <description>Forwards divided by released.</description>
    <externalId>false</externalId>
    <formula>if(iContactforSF__Released__c &gt; 0, iContactforSF__Forwards__c / iContactforSF__Released__c, 0)</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <inlineHelpText>The forward rate is the ratio of forwards per number of recipients.</inlineHelpText>
    <label>Forward Rate</label>
    <precision>18</precision>
    <required>false</required>
    <scale>2</scale>
    <trackTrending>false</trackTrending>
    <type>Percent</type>
</CustomField>
